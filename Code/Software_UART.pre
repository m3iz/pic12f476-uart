void InitSoftUART(void)
{
GP0 = 1;

TRISIO1 = 1;
TRISIO0 = 0;
}



unsigned char UART_Receive(void)
{



unsigned char DataValue = 0;


while(GP1==1);

_delay((unsigned long)(((1000000/1200))*(4000000/4000000.0)));
_delay((unsigned long)(((1000000/1200)/2)*(4000000/4000000.0)));

for ( unsigned char i = 0; i < 8; i++ )
{
if ( GP1 == 1 )
{
DataValue += (1<<i);
}

_delay((unsigned long)(((1000000/1200))*(4000000/4000000.0)));
}


if ( GP1 == 1 )
{
_delay((unsigned long)(((1000000/1200)/2)*(4000000/4000000.0)));
return DataValue;
}
else
{
_delay((unsigned long)(((1000000/1200)/2)*(4000000/4000000.0)));
return 0x000;
}
}




void UART_Transmit(const char DataValue)
{

# 65
GP0 = 0;
_delay((unsigned long)(((1000000/1200))*(4000000/4000000.0)));

for ( unsigned char i = 0; i < 8; i++ )
{

if( ((DataValue>>i)&0x1) == 0x1 )
{
GP0 = 1;
}
else
{
GP0 = 0;
}

_delay((unsigned long)(((1000000/1200))*(4000000/4000000.0)));
}


GP0 = 1;
_delay((unsigned long)(((1000000/1200))*(4000000/4000000.0)));
}
